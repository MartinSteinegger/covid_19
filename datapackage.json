  "resources": [
    {
      "profile": "tabular-data-resource",
      "name": "covid19_case_numbers",
      "encoding": "utf-8",
      "format": "csv",
      "mediatype": "text/csv",
      "path": "COVID19_Fallzahlen_CH_total_v2.csv",
      "schema": {
        "fields": [
          {
            "name": "date",
            "type": "date",
            "description": "Date of notification"
          },
          {
            "name": "time",
            "type": "time",
            "description": "Time of notification"
          },
          {
            "name": "abbreviation_canton_and_fl",
            "type": "string",
            "description": "Abbreviation of the reporting canton"
          },
          {
            "name": "ncumul_tested",
            "type": "integer",
            "description": "Reported number of tests performed as of date"
          },
          {
            "name": "ncumul_conf",
            "type": "integer",
            "description": "Reported number of confirmed cases as of date"
          },
          {
            "name": "new_hosp",
            "type": "integer",
            "description": "New hospitalisations since last date"
          },
          {
            "name": "current_hosp",
            "type": "integer",
            "description": "Reported number of hospitalised patients on date"
          },
          {
            "name": "current_icu",
            "type": "integer",
            "description": "Reported number of hospitalised patients in ICUs on date"
          },
          {
            "name": "current_vent",
            "type": "integer",
            "description": "Reported number of patients requiring invasive ventilation on date"
          },
          {
            "name": "ncumul_released",
            "type": "integer",
            "description": "Reported number of patients released from hospitals or reported recovered as of date"
          },
          {
            "name": "ncumul_deceased",
            "type": "integer",
            "description": "Reported number of deceased as of date"
          },
          {
            "name": "source",
            "type": "string",
            "description": "Source of the information"
          },
          {
            "name": "current_isolated",
            "type": "integer",
            "description": "Reported number of isolated persons on date; Infected persons, who are not hospitalised"
          },
          {
            "name": "current_quarantined",
            "type": "integer",
            "description": "Reported number of quarantined persons on date; Persons, who were in 'close contact' with an infected person, while that person was infectious, and are not hospitalised themselves"
          },
        ],
        "missingValues": [
          ""
        ]
      }
    }
  ]
